{
  "settings": {
    "adminPathPrefix": "_admin",
    "containerLabelText": "qwerty",
    "baseImage": "public.ecr.aws/lambda/nodejs:latest",
    "zipSourceDir": "/Users/tdc/dev/local/zips/",
    "zipTargetDir": "/Users/tdc/dev/local/zipruns",
    "includeCloudVars": true,
    "envVars": {
      "curenv": "local"
    }
  },
  "endpoints": [
    {
      "path": "/hello",
      "method": "GET",
      "function": {
        "name": "hello",
        "type": "zip",
        "file": "hello.zip",
        "entryPoint": "index.handler",
        "envVars": {
          "extraMessage": "Have a great day"
        },
        "internalPort": 8080
      } 
    },
    {
      "path": "/echo",
      "method": "POST",
      "function": {
        "name": "echo",
        "type": "image",
        "envVars": {},
        "imageName": "echo-lambda-image:latest",
        "externalPort": 9000,
        "internalPort": 8080  
      }
    },
    {
      "path": "/proxyjson",
      "method": "GET",
      "function": {
        "name": "proxyjson",
        "type": "passthru",
        "url": "https://httpbin.org/json",
        "method": "GET"  
      }
    },
    {
      "path": "/hotdogfiles",
      "method": "GET",
      "function": {
        "name": "getHotdogFiles",
        "type": "filesystem",
        "rootDir": "/Users/tdc/dev/local/_docs/samples/hotdogs",
        "entryPoint": "index.handler",
        "envVars": {},
        "externalPort": 9000,
        "internalPort": 8080  
      }
    },
    {
      "path": "/hotdogs",
      "method": "GET",
      "function": {
        "name": "getHotdogs",
        "type": "lambda",
        "host": "localhost",
        "externalPort": 9000,
        "internalPort": 8080  
      } 
    },
    {
      "path": "/tododbug",
      "method": "ANY",
      "function": {
        "name": "tododebug",
        "type": "lambda",
        "host": "localhost",
        "externalPort": 9000,
        "internalPort": 8080  
      } 
    },
   {
      "path": "/todo",
      "method": "ANY",
      "function": {
        "name": "todos",
        "type": "zip",
        "file": "todo.zip",
        "entryPoint": "index.handler",
        "envVars": {
          "DB_HOST": "host.docker.internal",
          "DB_USER": "fakeuser",
          "DB_PASSWORD": "fakepassword",
          "DB_NAME": "postgres",
          "DB_PORT": 5432
        },
        "internalPort": 8080
      } 
    }
  ]
}