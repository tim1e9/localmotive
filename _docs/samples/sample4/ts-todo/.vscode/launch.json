  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": ["--import", "tsx"],
      "program": "${workspaceFolder}/node_modules/trylam/trylam.js",
      "args": ["index.handler", ".", ".ts"],
      "env": {
        "DB_HOST": "localhost",
        "DB_USER": "admin",
        "DB_PASSWORD": "fakepassword",
        "DB_NAME": "postgres",
        "DB_PORT": "5432"
      }
    }
  ]
}