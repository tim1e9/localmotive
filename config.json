{
  "settings": {
    "adminPathPrefix": "_admin",
    "containerLabelText": "qwerty",
    "baseImage": "public.ecr.aws/lambda/nodejs:latest",
    "zipSourceDir": "/Users/tdc/dev/docker/zips/",
    "zipTargetDir": "/Users/tdc/dev/docker/zipruns"
  },
  "endpoints": [
    {
      "path": "/products",
      "method": "GET",
      "function": {
        "name": "getProducts",
        "type": "zip",
        "file": "getProducts.zip",
        "entryPoint": "index.handler",
        "envVars": {},
        "internalPort": 8080
      } 
    },
    {
      "path": "/hotdogs",
      "method": "GET",
      "function": {
        "name": "getHotdogs",
        "type": "lambda",
        "host": "localhost",
        "externalPort": 9000,
        "internalPort": 8080  
      } 
    },
    {
      "path": "/hotdogfiles",
      "method": "GET",
      "function": {
        "name": "getHotdogFiles",
        "type": "filesystem",
        "rootDir": "/Users/tdc/dev/docker/sample-lambdas/hotdogs",
        "entryPoint": "index.handler",
        "envVars": {},
        "externalPort": 9000,
        "internalPort": 8080  
      }
    },
    {
      "path": "/products/{productId}/price",
      "method": "GET",
      "function": {
        "name": "getProductPrice",
        "type": "passthru",
        "url": "https://www.google.com/",
        "method": "GET"  
      }
    },
    {
      "path": "/echo",
      "method": "GET",
      "function": {
        "name": "echo",
        "type": "image",
        "envVars": {},
        "imageName": "echo-lambda-image:latest",
        "externalPort": 9000,
        "internalPort": 8080  
      }
    },
    {
      "path": "/products/{productId}",
      "method": "DELETE",
      "function": {
        "name": "deleteProduct",
        "type": "zip",
        "file": "DeleteProduct.zip",
        "entryPoint": "index.handler",
        "envVars": {},
        "externalPort": 9000,
        "internalPort": 8080
      }
    }
  ]
}