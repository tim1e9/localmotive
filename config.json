{
  "settings": {
    "adminPathPrefix": "_admin",
    "containerLabelText": "qwerty",
    "baseImage": "public.ecr.aws/lambda/nodejs:latest",
    "zipSourceDir": "/Users/tdc/dev/docker/zips/",
    "zipTargetDir": "/Users/tdc/dev/docker/zipruns"
  },
  "endpoints": [
    {
      "path": "/products",
      "method": "GET",
      "functionName": "getProducts"
    },
    {
      "path": "/hotdogs",
      "method": "GET",
      "functionName": "getHotdogs"
    },
    {
      "path": "/hotdogfiles",
      "method": "GET",
      "functionName": "getHotdogFiles"
    },
    {
      "path": "/products/{productId}/price",
      "method": "GET",
      "functionName": "getProductPrice"
    },
    {
      "path": "/products/{productId}",
      "method": "PUT",
      "functionName": "setProductDetails"
    },
    {
      "path": "/echo",
      "method": "GET",
      "functionName": "echo"
    },
    {
      "path": "/products/{productId}",
      "method": "DELETE",
      "functionName": "deleteProduct"
    }
  ],
  "functions": {
    "getProducts": {
      "type": "zip",
      "file": "getProducts.zip",
      "entryPoint": "index.handler",
      "envVars": {},
      "externalPort": 9000,
      "internalPort": 8080
    },
    "getHotdogs": {
      "type": "lambda",
      "host": "localhost",
      "externalPort": 9000,
      "internalPort": 8080
    },
    "getProductPrice": {
      "type": "passthru",
      "url": "https://www.google.com/",
      "method": "GET"
    },
    "getHotdogFiles": {
      "type": "filesystem",
      "rootDir": "/Users/tdc/dev/docker/sample-lambdas/hotdogs",
      "entryPoint": "index.handler",
      "envVars": {},
      "externalPort": 9000,
      "internalPort": 8080
    },
    "setProductDetails": {
      "type": "filesystem",
      "rootDir": "/Users/tdc/dev/foo/bar",
      "entryPoint": "index.handler",
      "envVars": {},
      "externalPort": 9000,
      "internalPort": 8080
    },
    "echo": {
      "type": "image",
      "envVars": {},
      "imageName": "echo-lambda-image:latest",
      "externalPort": 9000,
      "internalPort": 8080
    },
    "deleteProduct": {
      "type": "zip",
      "file": "DeleteProduct.zip",
      "entryPoint": "index.handler",
      "envVars": {},
      "externalPort": 9000,
      "internalPort": 8080
    }
  }
}